<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox fillWidth="false" spacing="10.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="shapov.cointrack.controllers.albumModeles.PageManagerController">
      <Button mnemonicParsing="false" onMouseClicked="#onClickedAddPage" prefHeight="30.0" prefWidth="200.0" text="Добавить страницу">
         <VBox.margin>
            <Insets left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </Button>
      <Button mnemonicParsing="false" onMouseClicked="#onClickedEditPage" prefHeight="30.0" prefWidth="200.0" text="Изменить страницу">
         <VBox.margin>
            <Insets left="5.0" right="5.0" />
         </VBox.margin>
      </Button>
      <Button mnemonicParsing="false" onMouseClicked="#onClickedDeletePage" prefHeight="30.0" prefWidth="200.0" text="Удалить страницу">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" />
         </VBox.margin>
      </Button>
</VBox>
