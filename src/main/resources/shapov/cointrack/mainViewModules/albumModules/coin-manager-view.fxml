<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>

<VBox fillWidth="false" spacing="10.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="shapov.cointrack.controllers.albumModeles.CoinManagerController">
        <Button mnemonicParsing="false" onMouseClicked="#onClickedAddCoin" prefHeight="30.0" prefWidth="200.0" text="Добавить монету">
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </Button>
        <Button mnemonicParsing="false" onMouseClicked="#onClickedEditCoin" prefHeight="30.0" prefWidth="200.0" text="Изменить монету">
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </Button>
        <Button mnemonicParsing="false" onMouseClicked="#onClickedDeleteCoin" prefHeight="30.0" prefWidth="200.0" text="Удалить монету">
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </Button>
        <Label fx:id="statusCoin">
            <VBox.margin>
                <Insets bottom="30.0" />
            </VBox.margin>
        </Label>
        <Button fx:id="btReset" disable="true" mnemonicParsing="false" onMouseClicked="#onResetCoin" prefHeight="30.0" prefWidth="200.0" text="Сбросить действие" />
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </padding>
    </VBox>
