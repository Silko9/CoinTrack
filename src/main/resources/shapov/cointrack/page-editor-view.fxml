<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="shapov.cointrack.controllers.editControllers.PageEditorController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <Label text="Заголовок страницы" />
   <TextField fx:id="textName" prefWidth="200.0" />
   <AnchorPane>
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin>
      <Button fx:id="btAccept" mnemonicParsing="false" onMouseClicked="#onClickedAccept" prefHeight="30.0" text="Приянть" AnchorPane.leftAnchor="0.0" />
      <Button mnemonicParsing="false" onMouseClicked="#onClickedCancel" prefHeight="30.0" text="Отмена" AnchorPane.rightAnchor="0.0" />
   </AnchorPane>
</VBox>
